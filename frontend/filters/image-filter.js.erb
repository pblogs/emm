'use strict';

angular.module('app')
  .filter('imageFilter', function() {
    var images = {
      avatar: "<%= asset_path('vendor/no_avatar.jpg') %>",
      album: "<%= asset_path('vendor/no_cover.jpg') %>"
    };

    return function(url, type) {
      return url ? url : images[type];
    };
  });
